{
	"name": "df_investiment_funds_yearly_return",
	"properties": {
		"folder": {
			"name": "Serving"
		},
		"type": "MappingDataFlow",
		"typeProperties": {
			"sources": [
				{
					"dataset": {
						"referenceName": "ds_if_cvm_transformed_quotes",
						"type": "DatasetReference"
					},
					"name": "source1"
				}
			],
			"sinks": [],
			"transformations": [
				{
					"name": "window1"
				},
				{
					"name": "derivedColumn1"
				}
			],
			"scriptLines": [
				"source(output(",
				"          tp_fundo as string,",
				"          cnpj_fundo as string,",
				"          dt_comptc as date,",
				"          vl_total as double,",
				"          vl_quota as double,",
				"          vl_patrim_liq as double,",
				"          captc_dia as double,",
				"          resg_dia as double,",
				"          nr_cotst as integer,",
				"          ingestion_timestamp as date,",
				"          transform_timestamp as date",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     limit: 100,",
				"     ignoreNoFilesFound: true) ~> source1",
				"source1 window(over(cnpj_fundo),",
				"     asc(dt_comptc, true),",
				"     row_number = rowNumber(),",
				"          first_quota = first(vl_quota)) ~> window1",
				"window1 derive(rentabilidade_diaria = vl_quota/first_quota) ~> derivedColumn1"
			]
		}
	}
}